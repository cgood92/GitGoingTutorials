      <rdf:Description tsc:version="2.0.1" dc:date="2015-07-08 09:06:59 PM" dc:source="Camtasia Studio,8.5.2,enu" dc:title="Remotes" tscDM:firstFrame="Remotes_First_Frame.png" tscDM:originId="B300140D-EBAF-429A-AA8C-0B65B449E335" tscDM:project="Remotes">
         <xmpDM:duration xmpDM:scale="1/1000" xmpDM:value="114866"/>
         <xmpDM:videoFrameSize stDim:unit="pixel" stDim:h="720" stDim:w="1340"/>
         <tsc:langName>
            <rdf:Bag>
               <rdf:li xml:lang="en-US">English</rdf:li></rdf:Bag>
